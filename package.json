{
  "name": "ngx-text-diff-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build --configuration production",
    "build_report": "ng build --configuration production --stats-json",
    "test": "echo 1",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build ngx-text-diff --configuration production",
    "copy-license": "copyfiles ./projects/ngx-text-diff/LICENSE ./dist/ngx-text-diff",
    "copy-readme": "copyfiles ./projects/ngx-text-diff/README.md ./dist/ngx-text-diff",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "npm_pack": "cd dist/ngx-text-diff && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "pack": "ng-packagr -p projects/ngx-text-diff/ng-package.json & cd dist/ngx-text-diff & npm publish --access=public"
  },
  "engines": {
    "node": "22.0.0",
    "npm": "10.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^20.7.0",
    "@angular/animations": "^20.3.16",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.16",
    "@angular/compiler": "^20.3.16",
    "@angular/core": "^20.3.16",
    "@angular/forms": "^20.3.16",
    "@angular/platform-browser": "^20.3.16",
    "@angular/platform-browser-dynamic": "^20.3.16",
    "@angular/router": "^20.3.16",
    "bootstrap": "^4.4.1",
    "codemirror": "^5.52.2",
    "copyfiles": "^2.4.1",
    "core-js": "^3.40.0",
    "diff-match-patch": "^1.0.4",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "rxjs": "~7.5.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.14",
    "@angular-devkit/core": "^20.3.14",
    "@angular/cli": "^20.3.14",
    "@angular/compiler-cli": "^20.3.16",
    "@angular/language-service": "^20.3.16",
    "@types/codemirror": "0.0.91",
    "@types/diff-match-patch": "^1.0.36",
    "@types/express": "^5.0.0",
    "@types/jasmine": "^5.1.5",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^22.10.5",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~5.5.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-packagr": "^20.3.2",
    "prettier": "^3.4.2",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.2",
    "tslint": "~6.1.3",
    "typescript": "^5.9.3",
    "webpack-bundle-analyzer": "^3.6.1"
  }
}